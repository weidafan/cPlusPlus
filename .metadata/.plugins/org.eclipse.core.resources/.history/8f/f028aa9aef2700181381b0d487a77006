#include "tester.h"
#include <string>
using namespace std;
int main() {
	string s = "hello world!!!!";
	int numThreads = 4;
	WHICH_PRINT wp = P3;
	int numTimesToPrint = 3;
	int millisecond_delay = 1000;
	startThreads(s, numThreads, wp, numTimesToPrint, millisecond_delay);
	setCancelThreads(false);
}

